{
  "key": "prezz",
  "name": "K15t Prezz",
  "description": "Visualize Confluence pages as slides in a presentation in Confluence.",
  "baseUrl": "https://cbromberg.github.io/prezz",
  "vendor": {
    "name": "K15t Labs",
    "url": "http://www.k15t.com"
  },
  "authentication": {
    "type": "none"
  },
  "version": "0.1",
  "modules": {
    "generalPages": [
      {
        "key": "show-prezz",
        "url": "/show-prezz.html?spaceKey={space.key}&pageId={page.id}",
        "location": "system.content.action",
        "name": {
          "value": "Show Prezz"
        }
      },
      {
        "key": "prezz",
        "url": "/index.html?spaceKey={space.key}&pageId={page.id}",
        "location": "system.content.action",
        "name": {
          "value": "Show Prezz2"
        }
      }
    ]
  },
  "scopes": ["read"]
}